# =============================================================================
# RAAA Environment Configuration Example
# =============================================================================
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# =============================================================================
# DeepSeek API Configuration
# =============================================================================
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com

# =============================================================================
# LangSmith Configuration (Optional - for tracing)
# =============================================================================
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=RAAA
LANGCHAIN_API_KEY=your_langsmith_api_key_here

# =============================================================================
# Development Mode (Optional)
# =============================================================================
# Set to 'true' to enable dev mode features like chain-of-thought display
RAAA_DEV_MODE=false

# =============================================================================
# Neo4j Configuration (Optional - for graph storage)
# =============================================================================
# If not configured, the system will fall back to JSON file-based graph storage
NEO4J_URI=neo4j://127.0.0.1:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password_here
# Set to 'true' to enable Neo4j graph storage instead of JSON files
NEO4J_ENABLED=false

# =============================================================================
# LangFuse Configuration (Optional - for observability and tracing)
# =============================================================================
# Sign up at https://cloud.langfuse.com to get your API keys
LANGFUSE_SECRET_KEY=sk-lf-your_secret_key_here
LANGFUSE_PUBLIC_KEY=pk-lf-your_public_key_here
LANGFUSE_HOST=https://cloud.langfuse.com
# Set to 'true' to enable LangFuse tracing
LANGFUSE_ENABLED=false
# Set to 'true' for debug logging
LANGFUSE_DEBUG=false
